{
    "name": "rowland/laravelblog",
    "description": "A Laravel 4 package to add a simple blog to a site",
    "keywords": ["laravel","blog"],
    "license": "MIT",
    "authors": [
        {
            "name": "Otieno Rowland",
            "email": "rowlandmtetezi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "thujohn/rss": "dev-master",
        "cviebrock/eloquent-sluggable": "1.0.*",
        "fzaninotto/faker": "1.4.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/migrations",
            "src/models",
            "src/models/entities",
            "src/models/repositories",
            "src/models/services",
            "src/extentions",
            "src/seeds"
        ],
        "psr-0": {
            "Rowland\\LaravelBlog\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
